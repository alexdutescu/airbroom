<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
    <div id="nume">
            <div class="{style.label}">Nume</div>
            <div class="{style.value}" ui:field="nume"/>
        </div>
    <div id="prenume">
            <div class="{style.label}">Prenume</div>
            <div class="{style.value}" ui:field="prenume"/>
        </div>
    <div id="adresa">
            <div class="{style.label}">Adresa</div>
            <div class="{style.value}" ui:field="adresa"/>
        </div>
    <div id="user">
            <div class="{style.label}">User</div>
            <div class="{style.value}" ui:field="user"/>
        </div>
    <div id="abonament">
            <div class="{style.label}">Abonament</div>
            <div class="{style.value}" ui:field="abonament"/>
        </div>
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
