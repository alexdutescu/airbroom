<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
    <div id="cost">
            <div class="{style.label}">Cost</div>
            <div class="{style.value}" ui:field="cost"/>
        </div>
    <div id="locDus">
            <div class="{style.label}">Loc Dus</div>
            <div class="{style.value}" ui:field="locDus"/>
        </div>
    <div id="locIntors">
            <div class="{style.label}">Loc Intors</div>
            <div class="{style.value}" ui:field="locIntors"/>
        </div>
    <div id="nume">
            <div class="{style.label}">Nume</div>
            <div class="{style.value}" ui:field="nume"/>
        </div>
    <div id="prenume">
            <div class="{style.label}">Prenume</div>
            <div class="{style.value}" ui:field="prenume"/>
        </div>
    <div id="cod">
            <div class="{style.label}">Cod</div>
            <div class="{style.value}" ui:field="cod"/>
        </div>
    <div id="zborDus">
            <div class="{style.label}">Zbor Dus</div>
            <div class="{style.value}" ui:field="zborDus"/>
        </div>
    <div id="zborIntors">
            <div class="{style.label}">Zbor Intors</div>
            <div class="{style.value}" ui:field="zborIntors"/>
        </div>
    <div id="persoana">
            <div class="{style.label}">Persoana</div>
            <div class="{style.value}" ui:field="persoana"/>
        </div>
    <div id="dusIntors">
            <div class="{style.label}">Dus Intors</div>
            <div class="{style.value}" ui:field="dusIntors"/>
        </div>
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
