<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
    <div id="avion">
            <div class="{style.label}">Avion</div>
            <div class="{style.value}" ui:field="avion"/>
        </div>
    <div id="cod">
            <div class="{style.label}">Cod</div>
            <div class="{style.value}" ui:field="cod"/>
        </div>
    <div id="numarKm">
            <div class="{style.label}">Numar Km</div>
            <div class="{style.value}" ui:field="numarKm"/>
        </div>
    <div id="data">
            <div class="{style.label}">Data</div>
            <div class="{style.value}" ui:field="data"/>
        </div>
    <div id="orasp">
            <div class="{style.label}">Orasp</div>
            <div class="{style.value}" ui:field="orasp"/>
        </div>
    <div id="orasd">
            <div class="{style.label}">Orasd</div>
            <div class="{style.value}" ui:field="orasd"/>
        </div>
    <div id="deschis">
            <div class="{style.label}">Deschis</div>
            <div class="{style.value}" ui:field="deschis"/>
        </div>
    <div id="escala">
            <div class="{style.label}">Escala</div>
            <div class="{style.value}" ui:field="escala"/>
        </div>
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
